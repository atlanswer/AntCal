{
  "extends": ["@antcal/tsconfig"],
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["vinxi/client", "vite/client"],
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "~/*": ["src/*"],
      "@/*": ["public/*"],
      "python/*": ["src/python/*"]
    },
    "tsBuildInfoFile": ".output/tsconfig.tsbuildinfo"
  },
  "include": ["public/**/*", "src/**/*"],
  "files": [
    "app.config.ts",
    "eslint.config.js",
    "postcss.config.js",
    "tailwind.config.js",
    "./src/components/figure/FigureSection.1.tsx"
  ]
}
