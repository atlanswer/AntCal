{
  "extends": ["@antcal/tsconfig"],
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["vinxi/client", "vite/client"],
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "~/*": ["src/*"],
      "/*": ["public/*"]
    },
    "tsBuildInfoFile": ".output/tsconfig.tsbuildinfo"
  },
  "exclude": [".output", ".vinxi", "public", "venv"]
}
