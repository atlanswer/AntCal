{
  "cleanUrls": true,
  "framework": "solidstart-1",
  "trailingSlash": false,
  "functions": {
    "api/*.py": {
      "includeFiles": "api/**/*",
      "excludeFiles": "{.turbo,.vercel,.venv,.vinxi,node_modules,public,src}/**/*"
    }
  },
  "rewrites": [
    {
      "source": "/api",
      "destination": "/api/main"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/main"
    }
  ],
  "regions": ["hkg1"]
}
